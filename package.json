      "types": "./dist/*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup src --format=esm --dts --no-splitting --minify --clean"
  },
  "keywords": [
    "fp",
    "functional",
    "programming",
    "typescript"
  ],
  "author": "Segun Adebayo",
  "license": "ISC",
  "devDependencies": {
    "prettier": "^2.8.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
