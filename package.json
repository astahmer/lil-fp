{
  "name": "lil-fp",
  "version": "1.0.3",
  "description": "Functional programming utilities for TypeScript",
  "main": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src --format=esm,cjs --dts --minify --clean",
    "release": "pnpm changeset version && pnpm changeset publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore",
    "prepublishOnly": "pnpm build"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "fp",
    "functional",
    "programming",
    "typescript"
  ],
  "author": "Segun Adebayo",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "prettier": "^2.8.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "clean-package": "^2.2.0"
  }
}
